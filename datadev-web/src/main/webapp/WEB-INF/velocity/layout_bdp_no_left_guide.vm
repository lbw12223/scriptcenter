</script>

<script type="text/javascript">
    var CD =  '$!{bdpCommonData}';
    var commonData = {};
    if(CD != null && CD != ""){
        try{
            commonData = eval("(" + CD+")")
        }catch(e) {

        }
    }
    var bdpInfo = {
        common:commonData,
        bdpDomain: "$!{bdpDomain}",
        aceStaticVersion: "$!{aceStaticVersion}",
        topMenuList: [],
        leftMenuList: [],
        selectMenu: {
            value: '$!{selectMenu.value}',
            ext2: '$!{selectMenu.ext2}',
            name: '$!{selectMenu.name}'
        },
        userHolder: {
            name: '$!{userHolder.name}',
            erp: '$!{userHolder.erp}',
            userSysInfo: {
                imgUrl: '$!{userHolder.userSysInfo.imgUrl}' ? '$!{userHolder.userSysInfo.imgUrl}' : '$!{bdpDomain}' + '/statics/ace/avatars/avatar2.png'
            }
        },
        tt_hostIp: '$!{tt_hostIp}',
        tt_now: '$!{tt_now}'
    };

    /*顶部菜单*/
    (function () {
        var list = bdpInfo.topMenuList, item, iItem;
        #foreach($item in $productList)
            item = {};
            item.name = '$!{item.name}';
            item.list = [];
            #foreach($s in $item.list)
                iItem = {};
                iItem.right = $!{s.right};
                iItem.value = '$!{s.value}';
                iItem.name = '$!{s.name}';
                iItem.ext2 = '$!{s.ext2}';
                item.list.push(iItem);
            #end
            list.push(item);
        #end
    })();

    /*左侧菜单*/
    (function () {
        var list = bdpInfo.leftMenuList, item, iItem;
        #foreach($item in $leftMenuList)
            item = {};
            item.name = '$!{item.name}';
            item.value = '$!{item.value}';
            item.ext1 = '$!{item.ext1}';
            item.right = $!{item.right};
            item.list = [];
            #foreach($s in $item.list)
                iItem = {};
                iItem.right = $!{s.right};
                iItem.value = '$!{s.value}';
                iItem.name = '$!{s.name}';
                iItem.ext1 = '$!{s.ext1}';
                iItem.ext5 = '$!{s.ext5}';
                item.list.push(iItem);
            #end
            list.push(item);
        #end
    })();

</script>

<!-- 下面是每个页面单独需要使用的JS文件 -->
