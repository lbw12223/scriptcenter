<script type="text/javascript">
    var _erp = '$!{userHolder.erp}';
    var jaq = jaq || [];
    jaq.push(['account', 'JA2018_1321215']);
    jaq.push(['domain', 'ide.bdp.jd.com']);
    jaq.push(['erp_account', _erp]);  //ERP帐号，erp_account为字段名固定不变，test填入用户erp帐号(调用方自己传,若不传,会记录用户的京东pin)
    (function () {
        var ja = document.createElement('script');
        ja.type = 'text/javascript';
        ja.async = true;
        ja.src = '//wl.jd.com/joya.js';
        var s = document.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(ja, s);
    })()


</script>

