<style type="text/css">
    .script-run-history-Time{
        width: 180px !important;
    }
    .run-item {
        cursor: pointer;
        text-decoration: underline;
        margin-left: 10px;
    }
    .tmp-version{
        text-decoration: none !important;
    }

    .run-item:hover {
        color: #3286E9;
    }

    .run-item-one {
        margin-left: 20px !important;
    }
    </style>
<div class="script-run-history" style="margin-top: 20px;">

    <div class="bdp-form-group" style="height: 32px;margin-bottom: 10px">
        <label class="bdp-control-label script-run-history-label">运行ID：</label>
        <div class="script-run-history-time-content ">
            <input id="runId" class="" style="width: 260px;">
        </div>
        <label class="bdp-control-label script-run-history-label">集市：</label>
        <div class="script-run-history-time-content " style="width: 260px;">
            <select class="bdp-form-control" value="" name="marketLinuxUser" id="queryMarketSelect" placeholder="请选择集市"
                    autocomplete="off" style="width: 100%">
                <option value=""></option>
            </select>
        </div>
        #if(${isClusterAdmin} == 'true')
            <label class="bdp-control-label script-run-history-label">运行人：</label>
            <div class="script-run-history-time-content " style="width: 260px">
                <input class="bdp-form-control" id="operatorSelect" autocomplete="off" style="width: 100%">
            </div>
        #end
        <a class="bdp-btn bdp-btn-primary" id="run-history-query" style="margin-top: -5px;margin-left: 10px;">查询</a>

    </div>




    <div class="bdp-form-group" style=";margin-bottom: 10px;margin-top: -7px">
        <label class="bdp-control-label script-run-history-label">状态：</label>
        <div class="script-run-history-content">
            <input class="script-run-history-checkbox run-history-checkbox" type="checkbox" value="0" ><span class="checkbox-span">成功</span>
            <input class="script-run-history-checkbox run-history-checkbox" type="checkbox" value="3" ><span class="checkbox-span">失败</span>
            <input class="script-run-history-checkbox run-history-checkbox" type="checkbox" value="4" ><span class="checkbox-span">强制终止</span>
            <input class="script-run-history-checkbox run-history-checkbox" type="checkbox" value="2" ><span class="checkbox-span">执行中</span>

        </div>
    </div>
    <div class="bdp-form-group" style="height: 32px;margin-bottom: 10px">
        <label class="bdp-control-label script-run-history-label">开始时间：</label>
        <div class="script-run-history-time-content">
            <input id="runStartTimeFrom" autocomplete="off" class="script-run-history-Time date-picker start-time">
            <span class="icon-calendar"></span>
        </div>
        <label class="bdp-control-label script-run-history-period">--</label>
        <div class="script-run-history-time-content">
            <input id="runStartTimeTo" autocomplete="off" class="script-run-history-Time date-picker end-time">
            <span class="icon-calendar"></span>
        </div>

        <label class="bdp-control-label script-run-history-label">结束时间：</label>
        <div class="script-run-history-time-content">
            <input id="runEndTimeFrom" autocomplete="off" class="script-run-history-Time date-picker start-time">
            <span class="icon-calendar"></span>
        </div>
        <label class="bdp-control-label script-run-history-period">--</label>
        <div class="script-run-history-time-content">
            <input id="runEndTimeTo" autocomplete="off" class="script-run-history-Time date-picker end-time">
            <span class="icon-calendar"></span>
        </div>
    </div>


    <a class="bdp-btn bdp-btn-default" id="stop-mutil" style="margin-left: 25px;">强制停止</a>

    <div class="jd-table-parent" id="run-jd-table-parent" style="margin-top:20px;  margin-left:20px;width: 100%;height:380px;overflow: hidden;background: #31353C;">
        <table class="jd-table table table-hover " id="run-grid-table" style="background: #272A2F;">
        </table>
        <div id="run-grid-pager">

        </div>
    </div>


</div>
