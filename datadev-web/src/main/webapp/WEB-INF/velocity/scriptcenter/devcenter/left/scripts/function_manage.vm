<div class="function-manage-tab left_manage" style="position: relative;">
    <div class="content function-content">
        <div style="width: 100%; width: 100%;padding: 5px 10px;height: 45px;background: white; box-shadow: 0 2px 10px 0 rgba(0, 0, 0, 0.1);">
            <input type="text" id="searchFunction" placeholder="请输入函数名称、参数说明"
                   style="width: 100%;height: 30px; width: 100%;text-indent: 5px;"/>
            <span class="bdp-icon ide-search" id="searchFunctionFlag" style="position: absolute;right: 17px;top: 12px;"></span>
        </div>
        <div style="    padding: 0px 10px;
    position: fixed;
    top: 45px;
    right: 0px;
    width: 100%;
    height: 100%;
    overflow: auto;
    bottom: 0px;">
            <ul id="funcT" class="ztree" style="">
            </ul>
        </div>

    </div>
</div>

<div class="bdp-modal " id="functionShowModal" role="dialog" aria-hidden="true"
     style="overflow:hidden;z-index: 9990 !important;">
    <div class="bdp-modal-dialog " style="width: 680px;">
        <div class="bdp-modal-content "> <!--这是弹出框的头部-->
            <div class="bdp-modal-header"
                 style="height: 45px !important;padding: 15px 20px;margin: 0px;border-bottom: 1px solid #44494F">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                        aria-hidden="true">&times;</span></button>
                <h4 class="bdp-modal-title">函数</h4>
            </div> <!--这是弹出框的主要内容-->
            <div class="bdp-modal-body account-body" style="width: 680px;height: 500px;position: relative;">
                <div class="funtionToolDiv">
                    <span id="funcName"></span>
                    <a class="bdp-btn bdp-btn-primary" id="copyFunc">复制</a>
                </div>
                <div>
                    <table>
                        <thead>
                        <tr>
                            <td> 属性名</td>
                            <td>属性信息</td>
                        </tr>
                        </thead>
                        <tbody>
                        <tr class="commonTr">
                            <td>函数名称</td>
                            <td id="funcTrName"></td>
                        </tr>
                        <tr class="commonTr">
                            <td>责任人</td>
                            <td id="funcTrOwner"></td>
                        </tr>
                        <tr class="commonTr">
                            <td>函数类型</td>
                            <td id="funcTrType"></td>
                        </tr>
                        <tr class="commonTr">
                            <td>修改人</td>
                            <td id="funcTrMender"></td>
                        </tr>
                        <tr class="commonTr">
                            <td>修改时间</td>
                            <td id="funcTrModified"></td>
                        </tr>
                        <tr class="expTr">
                            <td>命令格式</td>
                            <td><textarea id="expText" readonly></textarea></td>
                        </tr>
                        <tr class="descTr">
                            <td>参数说明</td>
                            <td><textarea id="descText" readonly></textarea></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div> <!--这是弹出框的底部内容-->
        </div>
    </div>
</div>

