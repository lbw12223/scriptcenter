<div id="backgroundSearch" style=" margin: auto;
    position: absolute;
    display: none;
    top: -45px;
    left: 0;
    bottom: 0;
    right: 0;
    z-index: 9999;
    background: rgba(0, 0, 0, 0.5) !important; /* 兼容ie几不知道，好像ie5 */
    background: #000;
    filter: Alpha(opacity=60); ">
    <div id="searchDiv">
        <div>
            <div style="padding: 10px 10px 0px 10px;">
                  <span id="scriptTypeSelectSpan" style="color: #A5AEBD;font-size: 14px;">脚本类型：</span>
                  <span id="filTypes">
                        #foreach($item in $!scriptTypeMap.entrySet())
                            <label for="$!{item.value}" style="margin-right: 5px;">
                                <input id="$!{item.value}" data-code = "$!{item.key}" data-type="$!{item.value}" style="margin-left:2px !important;" type="checkbox" checked/>
                                <span style="margin-left:5px !important;">$!{item.value}</span>
                            </label>
                        #end

                       #*    <span class="fileType" data-code="-1" data-type="all">all</span>*#
                 </span>

            </div>
        </div>
        <div class="field">
            <span class="bdp-icon search ide-search pop-title" data-title="搜索" style=" left: 8px;top: 24px; position: relative;"></span>
            <input type="text" name="searchName" id="searchName" placeholder="脚本名" style="text-indent: 15px;"  maxlength="40"/>
        </div>
    </div>
</div>
