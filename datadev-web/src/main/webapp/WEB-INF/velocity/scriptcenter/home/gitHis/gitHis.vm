 <link rel="stylesheet" href="/scriptcenter/statics/treeGrid/css/jquery.treegrid.css">


<input type="hidden" id="gitProjectId" value="${gitProjectId}">
<input type="hidden" id="gitProjectFilePath" value="${gitProjectFilePath}">
<input type="hidden" id="isDir" value="${isDir}">


<div  id="gitHisDiv" style="background-color: #2F3338;height: 100%; width: 100%;padding-top: 30px;">

    <form class="bdp-form-horizontal bdp-col-xs-12">

        <div class="bdp-form-group">

            <label class="bdp-col-xs-2 bdp-control-label"  for="keyword">关键字：</label>
            <div class="bdp-col-xs-3 bdp-control-content">
                <input class="bdp-col-xs-12 bdp-form-control" autocomplete="off" placeholder="请输入提交信息、提交ID" name="keyWord" id="keyWord">
            </div>

            <label class="bdp-col-xs-2 bdp-control-label" for="erp">提交人：</label>
            <div class="bdp-col-xs-3 bdp-control-content">
                <select id="selectUploadErp" class="bdp-col-xs-12" style="width: 30%;" >
                    <option value=""></option>
                </select>
            </div>
            <div class="bdp-col-xs-2 bdp-control-content">
                <a class="bdp-btn bdp-btn-primary " id="history-query">查询</a>
            </div>

        </div>
        <div class="bdp-form-group">
            <div class="" id="run-jd-table-parent" style="height: 400px;width: 900px;overflow: hidden;">

                <table class="jd-table table table-hover bdp-col-xs-12" id="run-grid-table" >
                </table>
                <div id="run-grid-pager">

                </div>
            </div>
        </div>


      #*  <div class="bdp-form-group" style="text-align: center;">
            <a class="bdp-btn bdp-btn-primary" id="addTemplateBtn">添加模板</a>
            <a class="bdp-btn bdp-btn-default" id="closeTemplateBtn">关闭</a>
        </div>*#

    </form>

</div>

