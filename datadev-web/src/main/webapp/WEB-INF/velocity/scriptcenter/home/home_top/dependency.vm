#set($layout="layout_blank.vm")
#set($page_title="dependency")
<link type="text/css" rel="stylesheet" href="/scriptcenter/statics/pages/home/home_top/dependency.css?dc=$!{staticVersion}">
<link rel="stylesheet" type="text/css"
      href="/scriptcenter/statics/zTree/css/zTreeStyle/zTreeStyle.css?_dc=$!{staticVersion}">
<input type="hidden" id="gitProjectId" value="${gitProjectId}">
<input type="hidden" id="gitProjectFilePath" value="${gitProjectFilePath}">
<div class=" account-body" style="width: 100%;height:100%;position: relative;">
    <form id="packZipForm" style="height: 100%;position: relative">
        <div style="width: 100%;position: absolute;top: 0px;bottom: 80px" class="bdp-form-group ">
            <div style="width: 50%;height: 100%;float: left;position: relative">
                <div class="leftBodyDiv ztreeDiv bdp-form-control" style="position: absolute;width: auto;left: 0px;right: 10px">
                    <div class="file-title">文件列表</div>

                    <ul id="leftPackZtree" class="ztree" style="position:absolute;top: 35px;bottom: 0px;overflow: auto;    width: 100%;"></ul>

                </div>
            </div>
            <div style="width: 50%;height: 100%;float: left;position: relative">
                <div class="rightBodyDiv ztreeDiv bdp-form-control" style="position: absolute;width: auto;left:10px;right: 0px;">
                    <div class="file-title">已选择项</div>

                    <ul id="rightPackZtree" class="ztree" style="position:absolute;top: 35px;bottom: 0px;overflow: auto;    width: 100%;"></ul>

                </div>
            </div>
        </div>
        <div style="width: 100%;position: absolute;height: 30px;line-height: 30px;bottom: 40px;" class="bdp-form-group ">
            <label style="width: 50px;text-align: right">启动项：</label>
            <input id="startShellPath" style="margin-left: 10px;width: 436px;padding-left: 10px" class="bdp-disabled" value="$!{gitProjectFilePath}">
        </div>
    </form>
</div> <!--这是弹出框的底部内容-->
<div class="dependencyBottom" style="">
    <a class="bdp-btn bdp-btn-primary" id="pack2Save">确定</a>
    <a class="bdp-btn bdp-btn-primary" id="pack2zip">确定并发布成zip包</a>
    <a class="bdp-btn bdp-btn-default" id="closeDia">取消</a>
</div>

<script src="/scriptcenter/statics/pages/map.js?_dc=$!{staticVersion}"></script>
<script src="/scriptcenter/statics/art/js/jquery.artDialog.js?_dc=$!{staticVersion}"></script>
<script src="/scriptcenter/statics/art/plugins/iframeTools.js?_dc=$!{staticVersion}"></script>
<script src="/scriptcenter/statics/zTree/js/jquery.ztree.all.js?_dc=$!{staticVersion}"></script>
<script src="/scriptcenter/statics/pages/home/scriptType.js?_dc=$!{staticVersion}"></script>
<script src="/scriptcenter/statics/pages/home/home_top/dependency.js?_dc=$!{staticVersion}"></script>
